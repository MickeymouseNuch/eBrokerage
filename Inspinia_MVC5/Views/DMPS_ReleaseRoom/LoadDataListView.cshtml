<div class="wrapper wrapper-content" style="padding-top:0px">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <!--<div class="ibox-title" style="padding-top:10px">-->
                <ul class="nav nav-tabs">
                    <li class="tabshow " data-tab="Lead" id="Lead"><a data-toggle="tab">รายการสัญญาเช่าห้องชุด</a></li>
                    @*<li class="tabshow " data-tab="Cancel" id="Cancel"><a data-toggle="tab">รายการยกเลิกสัญญาเช่าห้องชุด</a></li>*@
                </ul>

                <!-- @*<div class="ibox-tools">
        <a class="close-link">
            <i class="fa fa-times"></i>
        </a>
    </div>*@
