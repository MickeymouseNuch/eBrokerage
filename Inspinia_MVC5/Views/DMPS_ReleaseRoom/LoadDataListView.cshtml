                <div class="ibox-content">
                    <div class="row" id="" style="">
                        <div id="" class="col-lg-12" style="">
                            @{
                                var IsshowTab = ViewBag.IsshowTab;
                                var i = 1;

                                if (IsshowTab == "Lead")
                                {
                                    <table id="" class="table table-striped table-bordered table-hover dataTables-example dataTable" style="width:1800px;">
                                        <thead>
                                            <tr>
                                                <th>No.</th>
                                                
                                                <th style="width:150px"></th>
                                                <th style="width:80px">ตรวจห้องชุด</th>
                                                <th style="width:80px">ค่านายหน้า</th>
                                                <th style="width:100px">เลขที่เอกสาร</th>
                                                <th>โครงการ</th>
                                                <th>ห้อง</th>
                                                <th>ชั้น</th>
                                                <th>พื้นที่</th>
                                                <th>ประเภทห้อง</th>
                                                <th>ประเภทการเช่า</th>
                                                <th>สถานะ</th>
                                                <th>เจ้าของห้อง</th>
                                                <th>เบอร์ติดต่อ</th>
                                                <th>ราคาเช่า</th>
                                                <th>ระยะสัญญา</th>
                                                <th>วันที่ทำสัญญา</th>
                                                <th>วันที่สิ้นสุด</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @*@{int i = 1; }*@
                                            @foreach (Inspinia_MVC5.Models.DMPS.vw_Rental item in ViewBag.lstRental)
                                            {
                                            <tr class="IsRows" data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID">
                                                <td class="text-right"> @i </td>
                                                <td class="text-left">
                                                    <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" class="confirm-View btnVED btn-primary" title="แสดงข้อมูล" type="button"><i class="fa fa-search"></i></button>



                                                    @if (item.RentID != null && (item.RentStatus != 3 && item.RentStatus != 5))
                                                    {
                                                        if (item.RoomType != "SHOP")
                                                        {
                                                            <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-Print btnVED btn-warning" title="พิมพ์สัญญา" type="button"><i class="fa fa fa-print"></i></button>

                                                        }
                                                        else
                                                        {
                                                            <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-PrintShop btnVED btn-warning" type="button" title="พิมพ์สัญญา SHOP"><i class="fa fa-print"></i></button>

                                                        }
                                                        @*<button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-New btnVED btn-success" type="button" title="ทำสัญญาใหม่"><i class="fa fa-files-o"></i></button>*@
                                                        <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-Copy btnVED btn-info" type="button" title="ต่อสัญญา"><i class="fa fa-slideshare"></i></button>
                                                    }
                                                    else
                                                    {
                                                        if (item.RoomType != "SHOP")
                                                        {
                                                            <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-Print btnVED btn-warning" title="พิมพ์สัญญา" type="button"><i class="fa fa fa-print"></i></button>

                                                        }
                                                        else
                                                        {
                                                            <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-PrintShop btnVED btn-warning" type="button" title="พิมพ์สัญญา SHOP"><i class="fa fa-print"></i></button>

                                                        }
                                                    }

                                                    @* cancel-form *@
                                                    @if (item.UnitCls == "label-danger")
                                                    {
                                                        @*<button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-trashForm btnVED btn-warning" type="button" title="แบบอนุมัติยกเลิกหนังสือเช่าห้องชุด" style="background-color: chocolate;"><i class="fa fa-print"></i></button>*@
                                                    }

                                                    <button id="UploadDS" data-uid="@item.UnitID" data-cust="@item.DSContactID" data-dsid="@item.DepositID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-UploadDS btnVED btn-success" title="อัพโหลดเอกสาร" type="button"><i class="fa fa-file-text"></i></button>

                                                </td>

                                                <td class="text-left">
                                                    @if (item.RentID != null)
                                                    {
                                                        <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-PrintRNFUR btnVED btn-warning" title="พิมพ์ใบตรวจสอบสภาพห้องชุดของผู้เช่า (ก่อน)" type="button"><i class="fa fa fa-print"></i></button>
                                                        <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-PrinRNFURAFTER btnVED btn-warning" title="พิมพ์ใบตรวจสอบสภาพห้องชุดของผู้เช่า (จบสัญญา)" type="button"><i class="fa fa fa-print"></i></button>
                                                    }
                                                </td>

                                                <td class="text-left">
                                                    @if (item.CommissionChecked == true)
                                                    {
                                                        <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-PrintRNCOM btnVED btn-warning" title="พิมพ์ใบเสร็จรับเงินชั่วคราว (ค่าคอมมิชชั่น)" type="button"><i class="fa fa fa-print"></i></button>
                                                    }
                                                </td>
                                                <td style="color:blue"> @item.RentNo </td>

                                                <td> @item.ProjectName </td>
                                                <td> @item.UnitsCode </td>
                                                <td> @item.Floor </td>
                                                <td> @item.RoomArea </td>
                                                <td> @item.RoomType </td>
                                                <td><label class="label @item.RentCls">@item.RentTypeNameTH</label></td>
                                                <td><label class="label @item.UnitCls">@item.DescriptionTH</label></td>
                                                <td> @item.FirstName </td>
                                                <td> @item.Tel1 </td>
                                                <td> @item.RentPriceStart.GetValueOrDefault().ToString("###,##0.00") - @item.RentPriceEnd.GetValueOrDefault().ToString("###,##0.00") </td>
                                                <td> @item.RentPeriod </td>
                                                <td> @item.RentStartDate.GetValueOrDefault().ToString("dd-MM-yyyy") </td>
                                                <td> @item.RentEndDate.GetValueOrDefault().ToString("dd-MM-yyyy") </td>
                                            </tr>
                                                i++;
                                            }

                                        </tbody>
                                    </table>

                                }

                                <!--if (IsshowTab == "Cancel")
                                {
                                    <table id="" class="table table-striped table-bordered table-hover dataTables-example dataTable" style="width:1800px;">
                                        <thead>
                                            <tr>
                                                <th>No.</th>
                                                <th style="width:100px">เลขที่เอกสาร</th>
                                                <th style="width:150px"></th>
                                                
                                                <th>โครงการ</th>
                                                <th>ห้อง</th>
                                                <th>ชั้น</th>
                                                <th>พื้นที่</th>
                                                <th>ประเภทห้อง</th>
                                                <th>ประเภทการเช่า</th>
                                                <th>สถานะ</th>
                                                <th>เจ้าของห้อง</th>
                                                <th>เบอร์ติดต่อ</th>
                                                <th>ราคาเช่า</th>
                                                <th>ระยะสัญญา</th>
                                                <th>วันที่ทำสัญญา</th>
                                                <th>วันที่สิ้นสุด</th>
                                            </tr>
                                        </thead>
                                        <tbody>-->
                                            @*@{int i = 1; }*@
                                //foreach (Inspinia_MVC5.Models.DMPS.vw_CancelRent item in ViewBag.lstCancelRental)
                                            @*{
                                                <tr class="IsRows" data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID">
                                                    <td class="text-right"> @i </td>
                                                    <td style="color:blue"> @item.RentNo </td>
                                                    <td class="text-left">
                                                        <button data-dsid="@item.DepositID" data-cust="@item.DSContactID" data-uid="@item.UnitID" data-rntid="@item.RentID" data-furamt="@item.RoomFurnitureAmt" class="confirm-trashForm btnVED btn-warning" type="button" title="แบบอนุมัติยกเลิกหนังสือเช่าห้องชุด" style="background-color: chocolate;"><i class="fa fa-print"></i></button>
                                                    </td>

                                                    <td> @item.ProjectName </td>
                                                    <td> @item.UnitsCode </td>
                                                    <td> @item.Floor </td>
                                                    <td> @item.RoomArea </td>
                                                    <td> @item.RoomType </td>
                                                    <td><label class="label @item.RentCls">@item.RentTypeNameTH</label></td>
                                                    <td><label class="label @item.UnitCls">@item.DescriptionTH</label></td>
                                                    <td> @item.FirstName </td>
                                                    <td> @item.Tel1 </td>
                                                    <td> @item.RentPriceStart.GetValueOrDefault().ToString("###,##0.00") - @item.RentPriceEnd.GetValueOrDefault().ToString("###,##0.00") </td>
                                                    <td> @item.RentPeriod </td>
                                                    <td> @item.RentStartDate.GetValueOrDefault().ToString("dd-MM-yyyy") </td>
                                                    <td> @item.RentEndDate.GetValueOrDefault().ToString("dd-MM-yyyy") </td>
                                                </tr>
                                                i++;
                                            }*@

                                        <!--</tbody>
                                    </table>

                                }-->

                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



