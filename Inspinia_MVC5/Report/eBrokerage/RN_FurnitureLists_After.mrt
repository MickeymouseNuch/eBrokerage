        <Name>CheckRoomList</Name>
        <NameInSource>Connection1</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_RentID,RentID,8,0</value>
        </Parameters>
        <SqlCommand>SELECT MS_CHK.CheckRoomDesc , MS_CHK.CheckRoomID , MS_CHK.IsQty , MS_CHK.CheckRoomUnit ,  TRANS.ID,CASE WHEN ISNULL(TRANS.CheckRoomQty,'') = 0 THEN '' ELSE ISNULL(TRANS.CheckRoomQty,'') END  as CheckRoomQty , 
CAST( ISNULL(TRANS.IsBroken,0) as int) as IsBroken, CASE WHEN ISNULL(TRANS.BrokenRoomQty,0)  = 0 THEN '' ELSE ISNULL(TRANS.BrokenRoomQty,0)  END as BrokenRoomQty ,
	CASE WHEN TRANS.ID is null THEN 0 else 1 END AS Checkbox_Room 
FROM (
SELECT [CheckRoomID]
	,[CheckRoomDesc]
	,[IsQty]
	,Sort
	,ISNULL([CheckRoomUnit],'') as [CheckRoomUnit] FROM [PMdb].[dbo].[CheckRoomMasterTable] as MS_CHK )  as MS_CHK
LEFT JOIN (SELECT *
	FROM [PMdb].[dbo].[CheckRoomTrans]
	where CheckType = 3
	and ID = @RentID) as TRANS
ON MS_CHK.CheckRoomID = TRANS.CheckRoomID
where MS_CHK.CheckRoomID not in (1,2)
order by MS_CHK.Sort</SqlCommand>
