	[dbo].[fn_NumberToWordThai](RoomInsuranceQty) as TH_RoomInsuranceQty,
	[dbo].[fn_NumberToWordThai](RoomDepositQty) as TH_RoomDepositQty,
	dbo.fn_CurrencyToWordThai(ISNULL(RoomInsuranceAmt,0)) AS TH_RoomInsuranceAmt,
	dbo.fn_CurrencyToWordThai(ISNULL(RoomDepositAmt,0)) AS TH_RoomDepositAmt,
	dbo.fn_CurrencyToWordThai(ISNULL(MulctPrice,0)) AS TH_MulctPrice,
	CASE WHEN DS_UnExpireIDCard = 1 THEN '-'  ELSE dbo.[ThaiMMMMDate](DS_CitizenIssue) END AS TH_DS_CitizenIssue ,
