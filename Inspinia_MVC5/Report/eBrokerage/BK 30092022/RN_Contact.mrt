CASE WHEN RN_UnExpireIDCard = 1 THEN '-'  ELSE dbo.[ThaiMMMMDate](RN_CitizenExp) END AS TH_RN_CitizenExp 
	FROM [PMdb].[dbo].[vw_rpt_Rent]
	WHERE RentID = @RentID</SqlCommand>
