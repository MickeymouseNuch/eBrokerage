          <value>_x0040_DepositID,DepositID,8,50</value>
        </Parameters>
        <SqlCommand>SELECT *
	  ,CASE WHEN [RentPriceStart] &gt; 0  THEN CAST(FORMAT([RentPriceStart],'###,###,###,###,###') AS nvarchar(MAX)) + ' - ' + CAST(FORMAT([RentPriceEnd],'###,###,###,###,###') AS nvarchar(MAX)) ELSE '-'   END  AS NUM_RentPrice
	  ,CASE WHEN [SalePriceStart] &gt; 0  THEN CAST(FORMAT([SalePriceStart],'###,###,###,###,###') AS nvarchar(MAX)) + ' - ' + CAST(FORMAT([SalePriceEnd],'###,###,###,###,###') AS nvarchar(MAX)) ELSE '-'   END  AS NUM_SalePrice
	  ,CASE WHEN [RentPriceStart] &gt; 0  THEN dbo.fn_CurrencyToWordThai([RentPriceStart]) + ' ถึง ' + dbo.fn_CurrencyToWordThai([RentPriceEnd]) ELSE '-'   END  AS THAI_RentPrice
	  ,CASE WHEN [SalePriceStart] &gt; 0  THEN dbo.fn_CurrencyToWordThai([SalePriceStart]) + ' ถึง ' + dbo.fn_CurrencyToWordThai([SalePriceEnd]) ELSE '-'   END  AS THAI_SalePrice
		,dbo.ThaiMMMMDate(DepositDate) as TH_DepositDate
		,dbo.ThaiMMMMDate(DepositStart) as TH_DepositStart
		,dbo.ThaiMMMMDate(DepositEnd) as TH_DepositEnd
,RateComCopyContact
	FROM [dbo].[vw_rpt_Deposit]
	WHERE DepositID = @DepositID</SqlCommand>
